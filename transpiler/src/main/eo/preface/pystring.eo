+package preface
+alias pybool preface.pybool
+alias pyint preface.pyint
+alias stdout org.eolang.io.stdout
+alias sprintf org.eolang.txt.sprintf

[value] > pystring
  # Tests that $ = x
  [x] > eq
    pybool (value.eq (x.value)) > @

  [] > length
    pyint (value.length) > @

  [] > as-string
    pystring value > @

  [] > force
    value > @

  seq > @
    stdout (sprintf "dataizing value %s\n" value)
    value
